            div.logo.head__logo
                //a.logo__link(href="")
                svg(width="145" height="107")
                    use(xlink:href="#main-logo")
            aside.head__aside
                div.head__rows
                    div.head__row
                        ul.head__lang-list
                            li.head__lang-item
                                a.head__lang-link(href="")
                                    |Eng
                            li.head__lang-item
                                a.head__lang-link(href="")
                                    |中文
                        a.head__phone(href="tel:")
                            |+7 (35151) 4-38-45
                        button.head__feedback-button.js-popup-button(data-for="feedback")
                            |Связаться с нами
                    div.head__row
                        nav.main-nav
                            ul.main-nav__list
                                li.main-nav__item
                                    a.main-nav__link(href="")
                                        |О предприятии
